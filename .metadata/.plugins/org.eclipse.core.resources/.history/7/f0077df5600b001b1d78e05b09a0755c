	.syntax unified
	.cpu cortex-m4
	.thumb

.data
	infix_expr: .asciz "{-99+ [ 10 + 20-0] }"
	user_stack_bottom: .zero 128

.text
	.global main

main:
	bl stack_init
	ldr R0, =infix_expr
	bl pare_check

L: b L

stack_init:
	ldr R0, =user_stack_bottom
	msr msp, R0
	bx lr

pare_check:
	ldr R1, =infix_expr
	ldrb R2, [R1]
	ldrb R3, [R1, #4]
	bx lr
